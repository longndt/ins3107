@model List<Web1.Models.Student>

<table class="table table-info">
    <thead>
        <tr>
            <th colspan="3">Student List</th>
        </tr>
        <tr>
            <th>
                Name
            </th>
            <th>
                Age
            </th>
            <th>
                Image
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Age</td>
                <td>
                    <img src="@student.Image" width="100" height="100" />
                </td>
            </tr>
        }
    </tbody>
</table>